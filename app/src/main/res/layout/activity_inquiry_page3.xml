        android:layout_marginEnd="8dp"
        android:text="Cultural Diversity Preference"
        app:layout_constraintBottom_toTopOf="@+id/spinnerCultural"
        app:layout_constraintEnd_toStartOf="@+id/textViewIrish"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerSex" />

    <TextView
        android:id="@+id/textViewModern"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="60dp"
