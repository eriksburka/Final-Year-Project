        app:layout_constraintEnd_toStartOf="@+id/textViewOccupancy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinnerOccupancy" />

    <Spinner
        android:id="@+id/spinnerIrish"
